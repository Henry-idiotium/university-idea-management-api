{
  "https_port": 443,
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "uimdb": "<ConnectionString>"
  },
  "Sieve": {
    "CaseSensitive": false,
    "DefaultPageSize": 5,
    "MaxPageSize": 20,
    "ThrowExceptions": false,
    "IgnoreNullsOnNotEqual": true
  }
}